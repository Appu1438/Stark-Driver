{
  "expo": {
    "name": "Stark Driver",
    "slug": "stark-driver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/FullLogo.png",
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "stark-driver",
    "deepLinks": {
      "paths": [
        "wallet-success",
        "wallet-failed",
        "wallet-cancelled"
      ]
    },
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/logo/FullLogo.png",
          "dark": {
            "image": "./assets/images/logo/FullLogo.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 250
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/TT-Octosquares-Medium.ttf"
          ]
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/FullLogo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.adithyanskumar.starkdriver",
      "versionCode": 1,
      "enableSeparateBuildPerArchitecture": true,
      "enableProguardInReleaseBuilds": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDZ3-iM9suiGVssgSjEdApgQXpXmDgvVAc"
        }
      }
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/logo/FullLogo.png",
      "bundleIdentifier": "com.adithyanskumar.starkdriver",
      "config": {
        "googleMapsApiKey": "AIzaSyDZ3-iM9suiGVssgSjEdApgQXpXmDgvVAc"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "extra": {
      "eas": {
        "projectId": "b2519774-9cf7-484b-81b1-4c488f2c3356"
      }
    }
  }
}